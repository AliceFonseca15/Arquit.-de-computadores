
#Q10) Faça um programa que leia um caractere minúsculo
#     e imprima o seu equivalente maiúsculo.

.text
main:  addi $2, $0, 12
       syscall
       add $8, $0, $2
       
       addi $9, $8, -32
       
       addi $2, $0, 11
       add $4, $0, '\n'
       syscall
       
       addi $2, $0, 11
       add $4, $0, $9
       syscall
       
       
       addi $2, $0, 10
       syscall